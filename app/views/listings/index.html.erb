<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Space name</th>
      <th>Venue name</th>
      <th>Description</th>
      <th>Address line one</th>
      <th>Address line two</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Zipcode</th>
      <th>Timezone</th>
      <th>Seating</th>
      <th>Standing</th>
      <th>Boardrooms</th>
      <th>Classrooms</th>
      <th>Theater</th>
      <th>U shaped</th>
      <th>Cabaret</th>
      <th>Area type</th>
      <th>Area dimension</th>
      <th>Ceiling type</th>
      <th>Ceiling dimension</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.space_name %></td>
        <td><%= listing.venue_name %></td>
        <td><%= listing.description %></td>
        <td><%= listing.address_line_one %></td>
        <td><%= listing.address_line_two %></td>
        <td><%= listing.city %></td>
        <td><%= listing.state %></td>
        <td><%= listing.country %></td>
        <td><%= listing.latitude %></td>
        <td><%= listing.longitude %></td>
        <td><%= listing.zipcode %></td>
        <td><%= listing.timezone %></td>
        <td><%= listing.seating %></td>
        <td><%= listing.standing %></td>
        <td><%= listing.boardrooms %></td>
        <td><%= listing.classrooms %></td>
        <td><%= listing.theatre %></td>
        <td><%= listing.u_shaped %></td>
        <td><%= listing.cabaret %></td>
        <td><%= listing.area_type %></td>
        <td><%= listing.area_dimension %></td>
        <td><%= listing.ceiling_type %></td>
        <td><%= listing.ceiling_dimension %></td>
        <td><%= listing.user_id %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
